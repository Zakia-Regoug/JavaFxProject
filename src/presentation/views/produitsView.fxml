<?xml version="1.0" encoding="UTF-8"?>

<?import java.lang.*?>
<?import java.util.*?>
<?import javafx.scene.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>

<BorderPane xmlns="http://javafx.com/javafx"
            xmlns:fx="http://javafx.com/fxml"
            fx:controller="presentation.controllers.ProduitController"
            prefHeight="400.0" prefWidth="600.0">
    <top>
        <HBox>
            <children>
                <Label text="Nom : "></Label>
                <TextField fx:id="textnom"></TextField>
                <Button text="Ajouter" onAction="#addProduit"></Button>
                <Button text="Supprimer" onAction="#delProduit"></Button>
            </children>
        </HBox>
    </top>
    <center>
        <ListView fx:id="listView"></ListView>
    </center>
</BorderPane>
